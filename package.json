    "de-wb-logger": {
      "versions": {
        "0.0.1": "consumeLogger"
      }
    }
  },
  "configSchema": {
  },
  "package-deps": [
    "ink"
  ],
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/node-persist": "0.0.32",
