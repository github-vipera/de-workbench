    "express": "^4.15.3",
    "fs-extra": "^4.0.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "loophole": "^1.1.0",
    "moment": "^2.18.1",
    "node-persist": "^2.1.0",
    "path": "^0.12.7",
    "request": "^2.81.0",
    "tree-kill": "^1.1.0",
