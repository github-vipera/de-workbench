{
  "name": "de-workbench",
  "version": "0.0.1",
  "author": "Dynamic Engine Team @ Vipera Plc",
  "description": "Dynamic Engine Workbench",
  "keywords": [
    "javascript",
    "debugger",
    "debug",
    "atom",
    "repl",
    "bugs",
    "chrome",
    "browser",
    "node",
    "breakpoint",
    "console",
    "vipera",
    "dynamicengine",
    "cordova",
    "android"
  ],
  "license": "MIT",
  "repository": "https://github.com/github-vipera/de-workbench",
  "main": "./lib/main",
  "showWelcome": true,
  "scripts": {
    "start": "tsc -w",
    "compile": "tsc"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "configSchema": {
    "toolbarStyle": {
      "title": "Toolbar Style",
      "description": "Change toolbar panel position.",
      "type": "string",
      "default": "TopPanel",
      "enum": [
        {
          "value": "TopPanel",
          "description": "Top"
        },
        {
          "value": "BottomPanel",
          "description": "Bottom"
        },
        {
          "value": "HeaderPanel",
          "description": "Title"
        }
      ]
    },
    "showToolbarIcon": {
      "title": "DE WB Icon",
      "description": "Show DE Workbench icon on toolbar.",
      "type": "boolean",
      "default": true
    }
  },
  "package-deps": [
    "atomify",
    "zentabs"
  ],
  "dependencies": {
    "atom-package-deps": "^4.6.0",
    "atom-select-list": "^0.2.0",
    "cordova-common": "^2.0.3",
    "cordova-lib": "^7.0.1",
    "fs-extra": "^3.0.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "path": "^0.12.7",
    "request": "^2.81.0",
    "tree-kill": "^1.1.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/atom": "^0.0.38",
    "@types/lodash": "^4.14.62",
    "@types/node": "^7.0.8",
    "typescript": "^2.3.0"
  },
  "de-workbench": [
    {
      "schemeName": "Develop",
      "pluginName": "Node.js",
      "pluginOptions": {
        "binaryPath": "",
        "portNumber": "5858",
        "lauchArguments": [],
        "environmentVariables": {}
      }
    }
  ]
}
