{
  "name": "de-workbench",
  "version": "0.1.6",
  "author": "Dynamic Engine Team @ Vipera Plc",
  "description": "Dynamic Engine Workbench",
  "keywords": [
    "javascript",
    "debugger",
    "debug",
    "atom",
    "repl",
    "bugs",
    "chrome",
    "browser",
    "node",
    "breakpoint",
    "console",
    "vipera",
    "dynamicengine",
    "cordova",
    "android"
  ],
  "license": "MIT",
  "repository": "https://github.com/github-vipera/de-workbench",
  "main": "./lib/main",
  "showWelcome": true,
  "scripts": {
    "start": "tsc -w",
    "compile": "tsc"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "consumedServices": {
    "ink": {
      "versions": {
        "*": "consumeInk"
      }
    }
  },
  "providedServices": {
    "de-wb-plugins-provider": {
      "versions": {
        "0.0.1": "provideCordovaPluginsProvider"
      }
    },
    "de-wb-logger": {
      "versions": {
        "0.0.1": "provideLogger"
      }
    },
    "de-wb-projectManager": {
      "versions": {
        "0.0.1": "provideProjectManager"
      }
    },
    "de-wb-events": {
      "versions": {
        "0.0.1": "provideEventBus"
      }
    },
    "de-wb-servers": {
      "versions": {
        "0.0.1": "provideServerManager"
      }
    }
  },
  "configSchema": {
    "AndroidWSPort": {
      "title": "Android platform WS port",
      "description": "WebSocket port for Android",
      "type": "integer",
      "default": 3000,
      "minimum": 1,
      "maximum": 65535,
      "order": 1
    },
    "iOSWSPort": {
      "title": "iOS platform WS port",
      "description": "WebSocket port for iOS",
      "type": "integer",
      "default": 3001,
      "minimum": 1,
      "maximum": 65535,
      "order": 2
    },
    "BrowserWSPort": {
      "title": "Browser platform WS port",
      "description": "WebSocket port for Browser",
      "type": "integer",
      "default": 3002,
      "minimum": 1,
      "maximum": 65535,
      "order": 3
    },
    "BrowserEmulationPort": {
      "title": "Browser emulation port",
      "description": "Browser emulation server port",
      "type": "integer",
      "default": 8001,
      "minimum": 1,
      "maximum": 65535,
      "order": 4
    }
  },
  "package-deps": [
    "ink"
  ],
  "dependencies": {
    "@types/express": "^4.0.36",
    "@types/node-persist": "0.0.32",
    "apn": "^2.1.5",
    "atom-package-deps": "^4.6.0",
    "atom-select-list": "^0.2.0",
    "chokidar": "^1.7.0",
    "cli-truncate": "^1.1.0",
    "cordova-common": "^2.0.3",
    "cordova-lib": "^7.0.1",
    "express": "^4.15.3",
    "fs-extra": "^4.0.1",
    "guid": "0.0.12",
    "jquery": "^3.2.1",
    "jsoneditor": "^5.9.3",
    "lodash": "^4.17.4",
    "loophole": "^1.1.0",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "node-gcm": "^0.14.6",
    "node-json-db": "^0.7.3",
    "node-persist": "^2.1.0",
    "node-pty": "^0.7.0",
    "path": "^0.12.7",
    "provisioning": "^1.5.0",
    "request": "^2.81.0",
    "resize-observer-polyfill": "^1.4.2",
    "reverse-line-reader": "^0.2.6",
    "socket.io": "^1.7.3",
    "string-hash": "^1.1.3",
    "tail": "^1.2.2",
    "term.js": "0.0.7",
    "tree-kill": "^1.1.0",
    "trivialdb": "^2.3.0",
    "uuid": "^3.1.0",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17",
    "xterm": "^2.9.2"
  },
  "devDependencies": {
    "@types/atom": "^0.0.38",
    "@types/lodash": "^4.14.62",
    "@types/node": "^8.0.19",
    "devtron": "^1.4.0",
    "typescript": "^2.3.0"
  },
  "de-workbench": [
    {
      "schemeName": "Develop",
      "pluginName": "Node.js",
      "pluginOptions": {
        "binaryPath": "",
        "portNumber": "5858",
        "lauchArguments": [],
        "environmentVariables": {}
      }
    }
  ]
}
